FUNK render(x,y,r,m)
{
    VAR c = r*r;
    VAR b = y*y;
    VAR a = x*x;
    VAR l = a + b
    VAR _return = l >= c-m AND l <= c + m
}

//GF_WRITE_LN(render(2.01,0,2,1.1))
GF_WRITE("y")VAR s_y = GF_INPUT("INT")
GF_WRITE("x")VAR s_x = GF_INPUT("INT")
GF_WRITE("r")VAR s_r = GF_INPUT("INT")

VAR i = 0

WHILE i < s_y
{ 
    VAR j = 0
    WHILE j < s_x 
    {
        IF render(-1 * s_x/2+j, -1 * s_y/2+i,s_r,3)
        {
            GF_WRITE("O")
        }
        ELSE
        {
            GF_WRITE(" ")
        }
        j++
    }
    i++
    GF_WRITE_LN("")
}